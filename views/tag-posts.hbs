<h2>{{tag}}</h2>

{{#each posts}}
  <div class="card">
    <div class="card-body">
      <h2 class="card-title"><a
          href="/posts/user/{{user._id}}"
        >{{user.username}}</a>
        -
        {{title}}</h2>
      <img src="{{image}}" class="card-img-bottom" alt="{{title}}" />
    </div>
  </div>
  <br />
{{/each}}
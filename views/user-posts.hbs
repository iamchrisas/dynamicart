{{#each posts}}
  <div class="card">
    <div class="card-body">
      <h2 class="card-title">{{this.user.username}} - {{this.title}}</h2>
      <img src="{{this.image}}" class="card-img-bottom" alt="{{this.title}}" />
    </div>
  </div>
  <br />
{{/each}}